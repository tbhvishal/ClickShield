# Example environment variables for ClickShield
# Copy this file to `.env` and fill in with your own values.

# Google Safe Browsing API key (required for threat checks)
GOOGLE_SAFE_BROWSING_API_KEY=your_api_key_here

# Backend server port (default: 8001)
PORT=8001

# Frontend dev server port (default: 5173)
FRONTEND_PORT=5173

# Backend API URL for frontend
# For local development: http://localhost:8001
# For production/Vercel: leave empty (will use relative URLs)
VITE_BACKEND_API_URL=http://localhost:8001

