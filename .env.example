# Example environment variables for ClickShield
# Copy this file to `.env` and fill in with your own values.

# Google Safe Browsing API key (required for threat checks)
GOOGLE_SAFE_BROWSING_KEY=your_google_safe_browsing_api_key_here

# Backend server port (default: 8001)
PORT=8001

# Optional: Frontend dev server port (default: 5173)
FRONTEND_PORT=5173

# Optional: Backend API URL for frontend (default: http://localhost:PORT)
# VITE_BACKEND_API_URL=http://localhost:8001

