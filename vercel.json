{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "backend/src/api/check-url.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/check-url",
      "dest": "/backend/src/api/check-url.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
